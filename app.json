    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bohdan.salenik.rn-todo-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.bohdan.salenik.rn_todo_app",
      "versionCode": 1,
      "permissions": [
        "CAMERA"
      ]
    },
    "description": "",
    "githubUrl": "https://github.com/BohdanSalenik/rn-todoApp.git"
  }
}
